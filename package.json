    "eslint-plugin-vue": "^5.2.2",
    "jest": "^24.1.0",
    "lint-staged": "^8.1.2",
    "nodemon": "^1.18.9",
    "prettier": "^1.16.4",
    "vue-jest": "^3.0.3"
